{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "no-use-before-define": "off",
    "semi": "off",
    "arrow-body-style": 0,
    "react/self-closing-comp":"off",
    "react/jsx-props-no-spreading": "off",
    "eslint no-console":"off"
  }
}
